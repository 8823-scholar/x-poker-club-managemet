# Google Sheets API
# サービスアカウントのJSON認証情報（Base64エンコード）
GOOGLE_SERVICE_ACCOUNT_KEY=

# スプレッドシートID（URLの /d/ と /edit の間の文字列）
GOOGLE_SPREADSHEET_ID=

# Notion API
NOTION_API_KEY=

# Notion Database IDs（URLの末尾32文字、ハイフンを除く）
NOTION_AGENT_DB_ID=
NOTION_PLAYER_DB_ID=
NOTION_WEEKLY_SUMMARY_DB_ID=
NOTION_WEEKLY_DETAIL_DB_ID=
NOTION_WEEKLY_TOTAL_DB_ID=

# Notion Data Source IDs（データベースのリレーションプロパティから取得）
NOTION_AGENT_DATA_SOURCE_ID=
NOTION_PLAYER_DATA_SOURCE_ID=
NOTION_WEEKLY_SUMMARY_DATA_SOURCE_ID=
NOTION_WEEKLY_DETAIL_DATA_SOURCE_ID=
NOTION_WEEKLY_TOTAL_DATA_SOURCE_ID=

# 週次集金サマリーのテンプレートページID（オプション）
# 設定すると新規作成時にテンプレートのブロックがコピーされます
NOTION_WEEKLY_SUMMARY_TEMPLATE_PAGE_ID=
